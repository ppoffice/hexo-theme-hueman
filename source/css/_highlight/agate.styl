.highlight
    color: white
    background: #333
    .code
        .name,
        .strong
            font-weight: bold

        .code,
        .emphasis
            font-style: italic

        .tag
            color: #62c8f3

        .variable,
        .template-variable,
        .selector-id,
        .selector-class
            color: #ade5fc

        .string,
        .bullet
            color: #a2fca2

        .type,
        .title,
        .section,
        .attribute,
        .quote,
        .built_in,
        .builtin-name
            color: #ffa

        .number,
        .symbol,
        .bullet
            color: #d36363

        .keyword,
        .selector-tag,
        .literal
            color: #fcc28c

        .comment,
        .deletion,
        .code
            color: #888

        .regexp,
        .link
            color: #c6b4f0

        .meta
            color: #fc9b9b

        .deletion
            background-color: #fc9b9b
            color: #333

        .addition
            background-color: #a2fca2
            color: #333

        .highlight a
            color: inherit

        .highlight a:focus,
        .highlight a:hover
            color: inherit
            text-decoration: underline
