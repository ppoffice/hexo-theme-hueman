.highlight
    color: #363c69
    background:#b7a68e url(./brown-papersq.png)
    .code
        .keyword,
        .selector-tag,
        .literal
            color:#005599
            font-weight:bold

        .highlight,
        .subst
            color: #363c69

        .string,
        .title,
        .section,
        .type,
        .attribute,
        .symbol,
        .bullet,
        .built_in,
        .addition,
        .variable,
        .template-tag,
        .template-variable,
        .link,
        .name
            color: #2c009f

        .comment,
        .quote,
        .meta,
        .deletion
            color: #802022

        .keyword,
        .selector-tag,
        .literal,
        .doctag,
        .title,
        .section,
        .type,
        .name,
        .strong
            font-weight: bold

        .emphasis
            font-style: italic
